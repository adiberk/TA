<p id="notice"><%= notice %></p>

<h1>Officehours</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Ta</th>
      <th>Start</th>
      <th>End</th>
      <th>Online</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @officehours.each do |officehour| %>
      <tr>
        <td><%= officehour.course_id %></td>
        <td><%= officehour.ta_id %></td>
        <td><%= officehour.start %></td>
        <td><%= officehour.end %></td>
        <td><%= officehour.online %></td>
        <td><%= link_to 'Show', officehour %></td>
        <td><%= link_to 'Edit', edit_officehour_path(officehour) %></td>
        <td><%= link_to 'Destroy', officehour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Officehour', new_officehour_path %>
